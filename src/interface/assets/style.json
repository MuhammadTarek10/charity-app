{
	"QCustomSlideMenu": [{
		"name": "sideMenuFrame",
		"defaultSize": [{
			"width": 150,
			"height": "auto"
		}],
		"collapsedSize": [{
			"width": 0,
			"height": 0
		}],
		"expandedSize": [{
			"width": 150,
			"height": "auto"
		}],
		"toggleButton": [{
			"buttonName": "menuButton",
			"icons": [{
				"whenMenuIsCollapsed": ":/icons/icons/menu.png",
				"whenMenuIsExpanded": ":/icons/icons/arrow-right.png"
			}]
		}],
		"menuTransitionAnimation": [{
			"animationDuration": 2000,
			"animationEasingCurve": "Linear",
			"whenCollapsing": [{
				"animationDuration": 500,
				"animationEasingCurve": "Linear"
			}],
			"whenExpanding": [{
				"animationDuration": 500,
				"animationEasingCurve": "Linear"
			}]
		}]
		
	}]
}